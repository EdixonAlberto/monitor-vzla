{
  "name": "bot-discord",
  "description": "Bot de discord para monitorear el precio del dolar paralelo en Venezuela",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "nodemon --config",
    "start": "node ../../dist"
  },
  "nodemonConfig": {
    "ext": ".ts,.env",
    "watch": [
      "src/*"
    ],
    "verbose": false,
    "exec": "yarn build && yarn start"
  },
  "dependencies": {
    "@edixon/concord": "^2.0.0",
    "socket.io-client": "^4.5.3"
  }
}