{
  "compilerOptions": {
    "target": "ES2016",
    "lib": ["ESNext"],
    "module": "commonjs",
    "rootDir": ".",
    "moduleResolution": "node",
    "declaration": true,
    "composite": true,
    "outDir": "dist",
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "skipLibCheck": true
    // "baseUrl": "src",
    // "paths": {},
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
